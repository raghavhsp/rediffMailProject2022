<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Basic Sanity" verbose="1" parallel="tests" thread-count="1">


	<test name="Validate elements of Rediffmail login page in Chrome Browser">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="common.RediffMail.Test.VerifyObjectsOfLoginPage" />
		</classes>

	</test>

	<test name="Validate elements of Rediffmail login page in Edge Browser">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="common.RediffMail.Test.VerifyObjectsOfLoginPage" />
		</classes>
	</test>



	<test name="Validate elements of Rediffmail login page in Firefox Browser">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="common.RediffMail.Test.VerifyObjectsOfLoginPage" />
		</classes>
	</test>

</suite>
